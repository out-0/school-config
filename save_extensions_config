#!/bin/bash
# 1. Export GNOME Extension settings
dconf dump /org/gnome/shell/extensions/ > ~/dotfiles/school/extension_settings.conf

# 2. Export general GNOME UI settings (optional, for things like dark mode/fonts)
dconf dump /org/gnome/desktop/ > ~/dotfiles/school/gnome_desktop_settings.conf

echo "Rice saved to dotfiles! ğŸš"
